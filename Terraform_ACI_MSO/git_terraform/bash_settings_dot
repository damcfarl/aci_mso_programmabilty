# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias gs='git status'
alias gl='git log --all --graph --decorate'
alias gll='git log --all --graph --decorate --oneline'
alias tf='/usr/local/bin/terraform'
alias tp='/usr/local/bin/terraform plan'
alias ta='/usr/local/bin/terraform apply'
